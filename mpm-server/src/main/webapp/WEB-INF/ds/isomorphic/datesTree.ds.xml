<DataSource
    ID="datesTree"
    serverType="generic">
    <fields>
        <field name="id" type="integer" primaryKey="true"/>
        <field name="title" type="text"/>
        <field name="parentId" type="integer" foreignKey="datesTree.id"/>
        <field name="year" type="integer"/>
        <field name="month" type="integer"/>
        <field name="photoCount" type="integer"/>
    </fields>
    <serverObject lookStyle="new" className="org.mpm.server.pics.PicsDataSource"/>
</DataSource>
