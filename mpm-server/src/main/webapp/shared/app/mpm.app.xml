<Application>
    <rpcBindings>
        <ServerObject ID="meta" className="org.mpm.server.metas.ConfigDataSource">
            <visibleMethods>
                <method name="fetchConfig"/>
            </visibleMethods>
        </ServerObject>
        <ServerObject ID="pics" className="org.mpm.server.pics.PicsDataSource">
            <visibleMethods>
                <method name="count"/>
                <method name="emptyTrash"/>
            </visibleMethods>
        </ServerObject>
        <ServerObject ID="pics" className="org.mpm.server.pics.ProgressDataSource">
            <visibleMethods>
                <method name="getProgress"/>
            </visibleMethods>
        </ServerObject>
    </rpcBindings>
</Application>